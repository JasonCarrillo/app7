<?xml version="1.0" encoding="utf-8"?> 

<widget xmlns= "http://www.w3.org/ns/widgets"
xmlns:gap="http://phonegap.com/ns/1.0"
id="com.phonegap.app7"
version="1.0.0">

<name> app7 LLamar Apps  </name>

<description>
Aplicacion que LLama otras apps
</description>

<author href="https://build.phonegap.com" email="carrillo.osorio.fmatutino@gmail.com">
Carrillo Osorio Jason Raul
</author>

<!--personaliza la aplicacion y plataforma-->
<preference name="orientation" value="default"/>
<!--preference name="orientation" value="landscape"/>-->
<!--preference name="orientation" value="landscape"/>-->
<preference name="target-device" value="universal"/>
<preference name="fullscreen" value="true"/>
<preference name="android-installLocation" value="auto"/>
<preference name="permissions" value="INTERNET"/>


<!--Plugins-->
<!--Core Plugins-->
<plugin name="cordova-plugin-device" spec="~1.1.4"/>
<plugin name="cordova-plugin-vibration" spec="~2.1.3"/>
<plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>

<!--<plugin name="cordova-plugin-geolocation"source="npm"/>-->

<!--Â´Para accesar a Paginas Web-->
<allow-intent href="https://*/*"/>
<acces origin="https://*/*"/>
<allow-intent href="http//*/*"/>
<acces origin="http://*/*"/>
<plugin name="cordova-plugin-whitelist" source="npm"/>

<!--Colocar Icono app-->

<icon src="res/android/mdpi.png"/>
</widget>